@model IEnumerable<Order>

@if (Model.Count() > 0)
{
    <div class="row">
        <div class="col-md-3"></div>
        <div class="col-md-9">
            @foreach (Order order in Model)
            {
                <div class="my-3">
                    <partial name="_OrderDetails" model="@order" />
                </div>
            }
        </div>
    </div>
}
else
{
    <div class="alert alert-info my-3">
        <p class="lead">There is no any order.</p>
    </div>
}